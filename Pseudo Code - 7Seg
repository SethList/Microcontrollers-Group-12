This code is for the "7 segment display". Code assumes a counter (use counter from main 'upcount' code. Code also assumes a delay function. Code also assumes common cathode LED.

From schematic it looks as if:
	LED1 - PORTD, 0
	LED2 - PORTD, 1
	LED3 - PORTD, 2
	LED4 - PORTD, 3
	LED5 - PORTE, 5
	LED6 - PORTD, 4
	LED7 - PORTD, 5
	LED8 - PORTD, 6
	LED9 - PORTD, 7



	LDI Rd, 'counter'   ; define a register (any) that will be the counter
	
	CP Rd, 0x0000
	BRNE NOT_0
	CALL NUM_0
	
NOT_0:	CP Rd, 0x0001
	BRNE NOT_1
	CALL NUM_1

NOT_1:	CP Rd, 0x0010
	BRNE NOT_2
	CALL NUM_2

NOT_2:	CP Rd, 0x0011
	BRNE NOT_3
	CALL NUM_3

NOT_3:	CP Rd, 0x0100
	BRNE NOT_4
	CALL NUM_4

NOT_4:	CP Rd, 0x0101
	BRNE NOT_5
	CALL NUM_5

NOT_5:	CP Rd, 0x0110
	BRNE NOT_6
	CALL NUM_6

NOT_6:	CP Rd, 0x0111
	BRNE NOT_7
	CALL NUM_7

NOT_7:	CP Rd, 0x1000
	BRNE NOT_8
	CALL NUM_8

NOT_8:	CALL NUM_9
	Rd++		; Counter advances to next count


NUM_0:	SBI PORTD, 0	; Turns on necessary LEDs, waits some time, then
	SBI PORTD, 1	; turns them off
	SBI PORTD, 2
	SBI PORTD, 3
	SBI PORTD, 5
	SBI PORTD, 4
	SBI PORTD, 6
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 1
	CBI PORTD, 2
	CBI PORTD, 3
	CBI PORTD, 5
	CBI PORTD, 4
	CBI PORTD, 6
	CBI PORTD, 7
	RET

NUM_1:	SBI PORTD, 3
	SBI PORTD, 6
	SBI PORTD, 9
	CALL Delay
	CBI PORTD, 3
	CBI PORTD, 6
	CBI PORTD, 9
	RET	

NUM_2:	SBI PORTD, 0
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 7
	RET

NUM_3:	SBI PORTD, 0
	SBI PORTD, 1
	SBI PORTD, 2
	SBI PORTE, 5
	SBI PORTD, 4
	SBI PORTD, 5
	SBI PORTD, 6
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 1
	CBI PORTD, 2
	CBI PORTE, 5
	CBI PORTD, 4
	CBI PORTD, 5
	CBI PORTD, 6
	CBI PORTD, 7
	RET

NUM_4:	SBI PORTD, 0
	SBI PORTD, 2
	SBI PORTD, 3
	SBI PORTE, 5
	SBI PORTD, 4
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 2
	CBI PORTD, 3
	CBI PORTE, 5
	CBI PORTD, 4
	CBI PORTD, 7
	RET

NUM_5:	SBI PORTD, 0
	SBI PORTD, 2
	SBI PORTE, 5
	SBI PORTD, 5
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 2
	CBI PORTE, 5
	CBI PORTD, 5
	CBI PORTD, 7
	RET

NUM_6:	SBI PORTD, 0
	SBI PORTD, 2
	SBI PORTD, 3
	SBI PORTD, 4
	SBI PORTD, 5
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 2
	CBI PORTD, 3
	CBI PORTD, 4
	CBI PORTD, 5
	CBI PORTD, 7
	RET

NUM_7:	SBI PORTD, 0
	SBI PORTD, 1
	SBI PORTD, 2
	SBI PORTD, 4
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 1
	CBI PORTD, 2
	CBI PORTD, 4
	CBI PORTD, 7
	RET

NUM_8:	SBI PORTD, 0
	SBI PORTD, 1
	SBI PORTD, 2
	SBI PORTD, 3
	SBI PORTE, 5
	SBI PORTD, 4
	SBI PORTD, 5
	SBI PORTD, 6
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 0
	CBI PORTD, 1
	CBI PORTD, 2
	CBI PORTD, 3
	CBI PORTE, 5
	CBI PORTD, 4
	CBI PORTD, 5
	CBI PORTD, 6
	CBI PORTD, 7
	RET

NUM_9:	SBI PORTD, 1
	SBI PORTD, 2
	SBI PORTE, 5
	SBI PORTD, 4
	SBI PORTD, 7
	CALL Delay
	CBI PORTD, 1
	CBI PORTD, 2
	CBI PORTE, 5
	CBI PORTD, 4
	CBI PORTD, 7
	RET
